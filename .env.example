# Rocket.Chat Configuration
# Copy this file to .env and fill in your actual values

# Your Rocket.Chat server URL (without trailing slash)
ROCKET_CHAT_URL=https://your-instance.rocket.chat

# Your User ID from Rocket.Chat Personal Access Token
ROCKET_CHAT_USER_ID=your_user_id_here

# Your Auth Token from Rocket.Chat Personal Access Token
ROCKET_CHAT_AUTH_TOKEN=your_auth_token_here